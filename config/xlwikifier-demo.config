
db_path = /shared/preprocessed/ctsai12/multilingual/xlwikifier-mapdb
#,db_path = /shared/bronte/ctsai12/multilingual/xlwikifier-mapdb/xlwikifier-mapdb

# This is only used if you want to import a new dump or train a ranking model
dump_path = /shared/preprocessed/ctsai12/multilingual/wikidump/ 

use_wikisearch = true
search_cache = /shared/bronte/tac2015/mediawiki_cache

# Which entity ID to use in the output view
# Two options:
# "freebase" : FreeBase MID
# "enwiki" : English Wikipedia title
target_kb = enwiki

liblinear_path = liblinear-ranksvm-1.95

# Ranking models for wikificaiton
en_ranking_model = xlwikifier-data/models/ranker/default/en/ranker.model
es_ranking_model = xlwikifier-data/models/ranker/tac-es/ranker.model
zh_ranking_model = xlwikifier-data/models/ranker/tac-zh/ranker.model
de_ranking_model = xlwikifier-data/models/ranker/default/de/ranker.model
nl_ranking_model = xlwikifier-data/models/ranker/default/nl/ranker.model
tr_ranking_model = xlwikifier-data/models/ranker/default/tr/ranker.model
tl_ranking_model = xlwikifier-data/models/ranker/default/tl/ranker.model
yo_ranking_model = xlwikifier-data/models/ranker/default/yo/ranker.model
bn_ranking_model = xlwikifier-data/models/ranker/default/bn/ranker.model
ta_ranking_model = xlwikifier-data/models/ranker/default/ta/ranker.model

# NER models for each language
en_ner_config = config/ner/monolingual/en.config
es_ner_config = config/ner/monolingual/es.config
zh_ner_config = config/ner/monolingual/zh.config
de_ner_config = config/ner/monolingual/de.config
nl_ner_config = config/ner/monolingual/nl.config
tr_ner_config = config/ner/monolingual/tr.config
tl_ner_config = config/ner/monolingual/tl.config
yo_ner_config = config/ner/monolingual/yo.config
bn_ner_config = config/ner/monolingual/bn.config
ta_ner_config = config/ner/monolingual/ta.config

